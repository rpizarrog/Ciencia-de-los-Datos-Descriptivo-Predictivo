# Agrupar datos de aguacate con respectgo a tipo
# se asigna a una tabla data frame la agrpaci贸 de datos en funci贸n al tipo
tabla_tipo <- datos %>%
  group_by(tipo) %>%
  summarise(n=n() )


# Agrupar datos de aguacate con respectgo a region
# se asigna a una tabla data frame la agrpaci贸 de datos en funci贸n a region
tabla_region <- datos %>%
  group_by(tipo) %>%
  summarise(n=n() )
