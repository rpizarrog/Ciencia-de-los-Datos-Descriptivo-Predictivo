mil_edades <- rnorm(n = 1000, mean = 38.1, sd = 14.04)
hist(mil_edades, 
     main = "Las edades",
     sub = paste("Me", round(media, 2), "; Mediana", round(mediana, 2),
                 "; ds", round(ds, 2), "; Max", round(maximo, 2), "; Min", round(minimo, 2),
                 ";Cuartiles", round(q1, 2), ", ", round(q2, 2), ", ", round(q3, 2)),
     xlab = "Valores",
     ylab = "Frecuencia")
